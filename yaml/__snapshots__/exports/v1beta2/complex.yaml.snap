version: v1beta2
namespace: data-team-pipeline
entities:
    - id: bq-synq-dwh::petr::kernel_assets__stg_assets
      time_partitioning_column: created_at
      monitors:
        - id: c20b07ae-27f5-50ff-ab17-7b11bed097dd
